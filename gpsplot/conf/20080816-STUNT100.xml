<?xml version="1.0" encoding="iso-8859-1" standalone="yes" ?>
<config version="1.0">

    <general>
        <title>STUNT100</title>
        <url>http://www.stunt100.de/</url>
        <author>
            <name>Oliver Lau</name>
            <email>oliver@knallwade.de</email>
            <url>http://knallwade.de/</url>
        </author>
        <encoding>UTF-8</encoding>
    </general>

    <input>
        <elevation>
            <file>..\..\..\knallwade.de\data\gps\STUNT100-2008\20080816-STUNT100.gpx</file>
            <offset>+00:00:00</offset>
        </elevation>
        <heartrate>
            <file></file>
            <offset>+00:00:00</offset>
        </heartrate>
    </input>

    <calculations>
        <smoothing id="Original">
            <draw>true</draw>
            <gnuplotOption>with lines lw 2 lc rgb "#E77171"</gnuplotOption>
        </smoothing>

        <smoothing id="Threshold (3m)">
            <algorithm>threshold</algorithm>
            <param>3</param>
            <draw>false</draw>
            <gnuplotOption>with lines</gnuplotOption>
        </smoothing>

        <smoothing id="Douglas-Peucker (3m)">
            <algorithm>douglas_peucker</algorithm>
            <param>3</param>
            <draw>false</draw>
            <gnuplotOption>with points pt 5 ps 0.3</gnuplotOption>
        </smoothing>

        <smoothing id="Douglas-Peucker (5m)">
            <algorithm>douglas_peucker</algorithm>
            <param>5</param>
            <draw>false</draw>
            <gnuplotOption>with points pt 5 ps 0.7</gnuplotOption>
        </smoothing>

        <smoothing id="Douglas-Peucker (8m)">
            <param>8</param>
            <algorithm>douglas_peucker</algorithm>
            <draw>true</draw>
            <gnuplotOption>with points pt 5 ps 0.3</gnuplotOption>
        </smoothing>
    </calculations>

    <gnuplot>
        <executable>I:\Programme\gnuplot\bin\wgnuplot.exe</executable>
        <!-- --><executable>gnuplot</executable> -->
        <format>png small size 1024,768 enhanced interlace</format>
        <suffix>png</suffix>
        <xaxis>duration</xaxis>
        <speedInterval unit="seconds">120</speedInterval>
        <legend>true</legend>
        <elevation>
            <height>70</height>
            <range>
                <lo></lo>
                <hi></hi>
            </range>
        </elevation>
        <speed>
            <height>15</height>
            <range>
                <lo></lo>
                <hi></hi>
            </range>
            <average>true</average>
        </speed>
        <hr>
            <height></height>
            <range>
                <lo></lo>
                <hi></hi>
            </range>
            <average>true</average>
        </hr>
        <slope>
            <height>15</height>
            <range>
                <lo>-25</lo>
                <hi>25</hi>
            </range>
            <source>Douglas-Peucker (8m)</source>
        </slope>
    </gnuplot>

    <dump>
        <file>20080816-STUNT100.dump.txt</file>
    </dump>

    <kml>
        <file>20080816-STUNT100.kml</file>
        <start>true</start>
        <finish>true</finish>
        <ticks>5</ticks>
    </kml>

    <googlemaps>
        <file>20080816-STUNT100.js</file>
    </googlemaps>

</config>
